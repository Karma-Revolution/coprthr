
./gen clrpc.rpc



gcc -DHAVE_CONFIG_H -I. -I/usr/local/include   -pthread  -g -O2 -Wall -fno-strict-aliasing  -pthread -c -o util.o util.c

gcc -DHAVE_CONFIG_H -DXCL_REPORT -I. -I/usr/local/include   -pthread    -g -O2 -Wall -fno-strict-aliasing  -pthread -c -o clrpcd.o clrpcd.c

gcc -DHAVE_CONFIG_H -DXCL_REPORT -I. -I/usr/local/include   -pthread    -g -O2 -Wall -fno-strict-aliasing  -pthread -c -o clrpc_test_cli.o clrpc_test_cli.c

gcc -DHAVE_CONFIG_H -I/usr/local/include   -pthread    -g -O2 -Wall -fno-strict-aliasing  -pthread -c -o clrpc.gen.o clrpc.gen.c 


gcc -DHAVE_CONFIG_H -DXCL_REPORT -I/usr/local/include   -pthread    -g -O2 -Wall -fno-strict-aliasing  -pthread -c -o clrpc_common.o clrpc_common.c

gcc -DHAVE_CONFIG_H -DXCL_REPORT -I/usr/local/include   -pthread    -g -O2 -fPIC -Wall -fno-strict-aliasing  -pthread -c -o libclrpc.o libclrpc.c

#libclrpc.so: libclrpc.o
gcc -shared -Wl,-soname,libclrpc.so -o libclrpc.so  libclrpc.o 


gcc -o clrpcd clrpcd.o clrpc.gen.o -L/usr/local/lib -levent clrpc_common.o util.o -L/usr/local/browndeer/lib -locl

gcc -o clrpc_test_cli clrpc_test_cli.o clrpc.gen.o -L/usr/local/lib -levent -levent_pthreads clrpc_common.o util.o -L. -lclrpc


